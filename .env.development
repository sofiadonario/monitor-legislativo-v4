# Development Environment Configuration
# This file controls whether to use mock data or real API

# Use mock data for development (set to false to use real API)
VITE_USE_MOCK_DATA=true

# API Configuration (for when using real API)
VITE_API_URL=https://monitor-legislativo-v4-production.up.railway.app
VITE_API_VERSION=v1

# Development features
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# Cache Configuration
VITE_CACHE_ENABLED=false
VITE_CACHE_TTL=0

# Service Worker (disabled in dev)
VITE_SW_ENABLED=false