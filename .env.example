# Environment Configuration for Monitor Legislativo v4
# Copy this file to .env and update with your actual values

# Development/Production Mode
NODE_ENV=production
VITE_MODE=production

# API Configuration
VITE_API_BASE_URL=https://monitor-legislativo-v4-production.up.railway.app
VITE_API_TIMEOUT=30000

# Data Source Configuration  
VITE_USE_MOCK_DATA=false
VITE_FORCE_CSV_ONLY=true

# Feature Flags
VITE_ENABLE_REALTIME=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_EXPORT=true

# Database Configuration (Backend)
DATABASE_URL=postgresql://username:password@host:port/database
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key

# Cache Configuration (Backend)
REDIS_URL=redis://default:password@host:port
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_redis_token

# External API Keys (Backend)
PLANALTO_API_KEY=your_planalto_api_key
CAMARA_API_KEY=your_camara_api_key
SENADO_API_KEY=your_senado_api_key

# Monitoring and Logging
LOG_LEVEL=info
ENABLE_MONITORING=true

# Security
JWT_SECRET=your_jwt_secret_key
CORS_ORIGIN=https://sofiadonario.github.io

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100